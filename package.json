{
	"name": "localtunnel-for-vscode",
	"displayName": "LocalTunnel for VSCode",
	"description": "Easily share a web service on your local development machine without messing with DNS and firewall settings directly from VSCode",
	"author": {
		"name": "Mauricio Soares",
		"email": "m.soares2603@gmail.com",
		"url": "https://github.com/mauriciosoares"
	},
	"keywords": [
		"localtunnel",
		"ngrok",
		"tunnel",
		"http",
		"webhooks",
		"webservice",
		"dns"
	],
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:localtunnel-for-vscode.create-config",
		"onCommand:localtunnel-for-vscode.start",
		"onCommand:localtunnel-for-vscode.stop"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "localtunnel-for-vscode.create-config",
				"title": "Create Workspace Configuration",
				"category": "LocalTunnel"
			},
			{
				"command": "localtunnel-for-vscode.start",
				"title": "Start",
				"category": "LocalTunnel"
			},
			{
				"command": "localtunnel-for-vscode.stop",
				"title": "Stop",
				"category": "LocalTunnel"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"lint": "eslint src --ext ts",
		"test": "jest"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-typescript": "^7.12.7",
		"@types/glob": "^7.1.3",
		"@types/jest": "^26.0.19",
		"@types/localtunnel": "^2.0.0",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"babel-jest": "^26.6.3",
		"babel-polyfill": "^6.26.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"jest": "^26.6.3",
		"mocha": "^8.1.3",
		"ts-jest": "^26.4.4",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"localtunnel": "^2.0.0"
	}
}
